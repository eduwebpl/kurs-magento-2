<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <head>
        <remove src="Eduweb_Brand::css/file.css"/>
    </head>

    <body>
        <referenceContainer name="content">
            <block name="child_1_theme" template="Eduweb_Brand::view/custom.phtml" before="-">
                <arguments>
                    <argument name="code" xsi:type="string">theme child block</argument>
                </arguments>
            </block>
        </referenceContainer>

        <move element="model_y" destination="content" before="-"/>

        <referenceBlock name="model_x" remove="true"/>
    </body>


</page>
